# Environment Configuration Template
# Copy this file to .env.dev, .env.uat, or .env.prod and fill in your values

# Database
DATABASE_URL=jdbc:postgresql://localhost:5432/postgres
DB_USERNAME=postgres
DB_PASSWORD=your_password

# JWT
JWT_SECRET=your-secret-key-minimum-32-characters

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_anon_key

# ToyyibPay
TOYYIBPAY_SECRET_KEY=your_secret_key
TOYYIBPAY_CATEGORY_CODE=your_category
TOYYIBPAY_RETURN_URL=http://localhost:3000/payment-status
TOYYIBPAY_CALLBACK_URL=http://localhost:8080/api/webhooks/toyyibpay

# CORS
CORS_ORIGINS=http://localhost:3000
